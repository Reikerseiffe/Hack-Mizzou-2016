<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jeffreyruffolo.notevote">

    <uses-permission android:name="android.permission.INTERNET"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.jeffreyruffolo.notevote.EnterActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.jeffreyruffolo.notevote.CreateActivity" />
        <activity android:name="com.jeffreyruffolo.notevote.JoinActivity" />
        <activity android:name="com.jeffreyruffolo.notevote.PlaylistActivity" />
        <activity android:name="com.jeffreyruffolo.notevote.SearchActivity"></activity>
    </application>

</manifest>